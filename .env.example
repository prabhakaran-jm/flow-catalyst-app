# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Supabase Project URL
SUPABASE_URL=your-project-url.supabase.co

# Supabase Anon Key (public key)
SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Service Role Key (keep secret!)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database Connection String (for migrations)
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# AI Provider (for run-catalyst Edge Function)
# Options: openrouter, openai, anthropic, gemini
AI_PROVIDER=openrouter

# OpenRouter - Get key from https://openrouter.ai/keys
OPENROUTER_API_KEY=your-openrouter-api-key
OPENROUTER_MODEL=openrouter/free
OPENROUTER_MAX_TOKENS=1000
OPENROUTER_TEMPERATURE=0.7

# Gemini (Google AI Studio) - Free key at https://aistudio.google.com/apikey
# AI_PROVIDER=gemini
# GEMINI_API_KEY=your-gemini-api-key
# GEMINI_MODEL=gemini-2.0-flash
# GEMINI_MAX_TOKENS=500
# GEMINI_TEMPERATURE=0.7

# Local dev only - never set in production
# ALLOW_DEV_AUTH_BYPASS=true   # JWT workaround for local Supabase
# ALLOW_TEST_PLAN_OVERRIDE=true # Set Pro bypasses rate limit for custom coaches
